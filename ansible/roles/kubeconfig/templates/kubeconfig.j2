apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: {{ kubernetes_certificates_ca_path }}
    server: {{ kubernetes_master_ip }}
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: kubelet
  name: kubelet
current-context: kubelet
users:
- name: kubelet
  user:
    token: kubelet_password
